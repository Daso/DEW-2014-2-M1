<h1>Chat UPC</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</table>
  <ul id="chat_container">
    <% @messages.each do |message| %>
    <li>
       <%= message.user %> dijo: <%= message.content %>
    </li>
    <% end %>
  </ul>  

<%= render 'form' %>

<br>
